{
  "name": "webpack-react-typescript-eslint-prettier-boilerplate",
  "version": "1.0.0",
  "description": "A webpack boilerplate with React, TypeScript, Eslint and Prettier",
  "main": "index.js",
  "author": "Yogesh Kumar",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.js --progress",
    "build": "webpack --config config/webpack.prod.js",
    "prod": "http-server ./dist -a localhost -p 8005 -o",
    "format:check": "prettier --config .prettierrc --check src/**/*.{js,jsx,ts,tsx,json,css,scss,sass}",
    "format:checkLocal": "prettier --config .prettierrc --check --end-of-line auto src/**/*.{js,jsx,ts,tsx,json,css,scss,sass}",
    "format:fix": "prettier --config .prettierrc --write src/**/*.{js,jsx,ts,tsx,json,css,scss,sass}",
    "format:fixLocal": "prettier --config .prettierrc --write --end-of-line auto src/**/*.{js,jsx,ts,tsx,json,css,scss,sass}",
    "lint:check": "eslint",
    "lint:fix": "eslint --fix"
  },
  "keywords": [
    "webpack 5",
    "Babel 7",
    "React",
    "TypeScript",
    "eslint",
    "prettier",
    "scss"
  ],
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-arrow-functions": "^7.25.9",
    "@babel/plugin-transform-async-to-generator": "^7.25.9",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-object-rest-spread": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.12.7",
    "@eslint/js": "^9.23.0",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/react-router-dom": "^5.3.2",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^9.23.0",
    "eslint-plugin-react": "^7.37.4",
    "fork-ts-checker-webpack-plugin": "^6.0.8",
    "globals": "^16.0.0",
    "html-webpack-plugin": "^5.6.3",
    "http-server": "^14.1.1",
    "mini-css-extract-plugin": "^2.9.2",
    "path": "^0.12.7",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.3",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.28.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yogeshkumarasamy/monorepo_and_microfrontends.git"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "core-js": "^3.39.0",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^5.2.0",
    "whatwg-fetch": "^3.6.20"
  }
}
